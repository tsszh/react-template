{
  "name": "react-start-kit",
  "version": "0.0.1",
  "description": "Zehao's React Start Kit",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config build/webpack.config.prod.js",
    "quick:build:webpack": "cross-env NODE_ENV=production webpack --config build/webpack.config.prod.quick.js --display-chunks",
    "quick:build:detail:webpack": "cross-env NODE_ENV=production webpack --config build/webpack.config.prod.quick.js --display-chunks --display-modules",
    "build": "npm run clean && npm run build:webpack",
    "qb": "npm run clean && npm run quick:build:webpack",
    "qbd": "npm run clean && npm run quick:build:detail:webpack",
    "start": "node build/devServer.js",
    "start2": "node build/devServer2.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "filesaver.js": "^0.2.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.13.0",
    "papaparse": "^4.1.2",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.1",
    "react-addons-update": "^15.1.0",
    "react-css-modules": "^3.7.6",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "main": "lib/index.js",
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^3.1.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "cas-authentication": "0.0.8",
    "cookie-parser": "^1.4.3",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "eventsource-polyfill": "^0.9.6",
    "expose-loader": "^0.7.1",
    "express": "^4.13.4",
    "express-session": "^1.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.17.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "object-assign": "^4.1.0",
    "postcss-loader": "^0.9.1",
    "pre-commit": "^1.1.3",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
